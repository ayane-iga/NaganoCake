<h4>会員一覧</h4>

<table class="table">
	<tr>
		<td>会員ID</td>
		<td>氏名</td>
		<td>メールアドレス</td>
		<td>ステータス</td>
	</tr>

	<% @end_users.each do |end_user| %>

	<tr>
		<td>
			<%= end_user.id %>
		</td>
		<td>
			<%= end_user.last_name %>
			<%= end_user.first_name %>
		</td>
		<td>
			<%= end_user.email %>
		</td>
		<td>
			<% if end_user.is_valid %>
			<p>有効</p>
			<% else %>
			<p>退会済</p>
			<% end %>
		</td>
	</tr>

	<% end %>

</table>